{
  "grpcGateway": {
    "name": "example",
    "httpPort": 80,
    "grpcPort": 6080,
    "exitTimeout": "10s",
    "headers": ["X-Request-Id"],
    "validators": ["Default"],
    "privateIP": "",
    "hostname": "",
    "requestIDMetaKey": "x-request-id",
    "ContentType": "application/json",
    "usePascalNameLogKey": false,
    "usePascalNameErrKey": false,
    "marshalUseProtoNames": false,
    "marshalEmitUnpopulated": true,
    "unmarshalDiscardUnknown": true,
    "enableTrace": true,
    "enableMetric": true,
    "enablePprof": true,
    "trace": {
      "constTags": {
        "key1": "val1"
      }
    },
    "metric": {
      "buckets": [0.1, 1, 10, 100],
      "constLabels": {
        "key1": "val1"
      }
    },
    "rateLimiterHeader": "x-user-id",
    "rateLimiter": {
      "type": "RedisRateLimiterInstance"
    },
    "parallelControllerHeader": "x-user-id",
    "parallelController": {
      "type": "RedisTimedParallelControllerInstance"
    },
    "jaeger": {
      "serviceName": "example",
      "sampler": {
        "type": "const",
        "param": 1
      },
      "reporter": {
        "logSpans": false
      }
    }
  },
  "parallelController": {
    "redis": {
      "redis": {
        "addr": "127.0.0.1:6379"
      },
      "wrapper": {
        "name": "parallelController",
        "enableTrace": true,
        "enableMetric": true
      }
    },
    "defaultMaxToken": 3,
    "maxToken": {
      "123|/api.ExampleService/Echo": 1
    },
    "interval": "3s",
    "expiration": "10s"
  },
  "rateLimiter": {
    "redis": {
      "redis": {
        "addr": "127.0.0.1:6379"
      },
      "wrapper": {
        "name": "rateLimiter",
        "enableTrace": true,
        "enableMetric": true
      }
    },
    "qps": {
      "123|/api.ExampleService/Echo": 1
    }
  },
  "logger": {
    "grpc": {
      "level": "Info",
      "writers": [{
        "type": "Stdout",
        "options": {
          "formatter": {
            "type": "Json",
            "options": {
              "flatMap": true
            }
          }
        }
      }, {
        "type": "RotateFile",
        "options": {
          "filename": "log/app.rpc",
          "maxAge": "24h",
          "formatter": {
            "type": "Json",
            "options": {
              "flatMap": true
            }
          }
        }
      }]
    },
    "info": {
      "level": "Info",
      "writers": [{
        "type": "Stdout",
        "options": {
          "formatter": {
            "type": "Json",
          }
        }
      }, {
        "type": "RotateFile",
        "options": {
          "filename": "log/app.log",
          "maxAge": "24h",
          "formatter": {
            "type": "Json"
          }
        }
      }]
    }
  }
}